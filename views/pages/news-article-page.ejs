<main class="content news-article">
  <img src="<%= thumbnail %>" width="550px" height="350px" alt="vignette">
  <h1 class="title"><%= title %></h1>
  <div class="paragraphs">
    <% for (const paragraph of paragraphs) { %>
      <p><%= paragraph %></p>
    <% } %> 
  </div>
  <%- include("../comments"); -%>
</main>